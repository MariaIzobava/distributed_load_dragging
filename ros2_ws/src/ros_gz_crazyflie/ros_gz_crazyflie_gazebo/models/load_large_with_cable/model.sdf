    <visual name="hook_visual"><geometry><sphere><radius>0.005</radius></sphere></geometry></visual>
    <collision name="hook_collision"><geometry><sphere><radius>0.005</radius></sphere></geometry></collision>
  </link>
  <joint name="box_to_hook_x_joint" type="fixed">
    <parent>box_body</parent>
    <child>x_hook</child>
  </joint>

  <link name="x_middle_hook">
