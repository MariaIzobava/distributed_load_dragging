      <visual name='visual'>
        <geometry>
          <box>
            <size>0.4 0.4 0.4</size>
          </box>
        </geometry>
          <!--let's add color to our link-->
          <material>
            <ambient>0.0 0.0 1.0 1</ambient>
            <diffuse>0.0 0.0 1.0 1</diffuse>
            <specular>0.0 0.0 1.0 1</specular>
          </material>
      </visual>
      <collision name='collision'>
        <geometry>
          <box>
            <size>0.4 0.4 0.4</size>
          </box>
        </geometry>
        <surface> 
          <friction> 
            <ode>  
              <mu> 0.3 </mu>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>
  <link name="x_hook">
    <!-- <pose>0 0 0.2 0 0 0</pose>  -->
    <pose>0 0 0 0 0 0</pose> 
    <inertial>
      <!-- <mass>0.000001</mass> -->
      <mass>0.00001</mass>
        <inertia>
          <ixx>1e-9</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-9</iyy>
          <iyz>0</iyz>
          <izz>1e-9</izz>
