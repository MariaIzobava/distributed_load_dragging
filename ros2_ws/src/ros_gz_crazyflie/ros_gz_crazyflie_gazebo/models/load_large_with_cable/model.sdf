<model name="the_load">
  <link name="box_body">
    <pose>0.2 0 0.2 0 0 0</pose>
      <inertial> <!--inertial properties of the link mass, inertia matix-->
        <mass>0.03</mass>
        <inertia>
          <ixx>0.000798</ixx> 
          <!-- <ixx>0.0000000532</ixx> -->
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.000798</iyy>
          <!-- <iyy>0.0000000798</iyy> -->
          <iyz>0</iyz>
          <izz>0.000798</izz>
